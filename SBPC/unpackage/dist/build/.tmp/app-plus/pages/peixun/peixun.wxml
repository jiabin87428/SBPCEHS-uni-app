<view class="baseView"><view class="pxTitleView"><label class="pxTitleLabel">{{model.bt}}</label><label class="pxSubTitleLabel">{{"积分："+model.kjjf+"分"}}</label></view><image class="classImage" mode="aspectFit" src="{{classList.length>0?classList[currentIndex].src:''}}" data-event-opts="{{[['tap',[['viewPhoto',['$0'],['classList.'+currentIndex+'']]]]]}}" bindtap="__e"></image><label class="countLabel">{{currentIndex+1+"/"+classList.length}}</label><view class="controlBtnView"><button data-event-opts="{{[['tap',[['provClick',['$event']]]]]}}" class="{{[currentIndex>0?'buttonStyle':'buttonStyleDisable']}}" bindtap="__e">上一页</button><button data-event-opts="{{[['tap',[['nextClick',['$event']]]]]}}" class="{{[isCountFinish?'buttonStyle':'buttonStyleDisable']}}" bindtap="__e">{{nextPage}}</button></view></view>